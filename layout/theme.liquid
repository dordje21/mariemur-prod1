                dy = void 0,
                tx = 0,
                ty = 0,
                key = -1,
                scX = 1,
                scY = 1;
        const followMouse = function followMouse() {
            key = requestAnimationFrame( followMouse );
            if ( !x || !y) {
                x = xmouse;
                y = ymouse;
            } else {
                dx = ( xmouse - x ) * 0.125;
                dy = ( ymouse - y ) * 0.125;
                if ( Math.abs( dx ) + Math.abs( dy ) < 0.1 ) {
                    x = xmouse;
                    y = ymouse;
                } else {
                    x += dx;
                    y += dy;
                    scX = Math.abs(1 - Math.abs( dx ) / 50);
