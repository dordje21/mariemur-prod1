<script>
    let id=0;
    $( document ).on( 'click', '.js-show-waitlist', function() {
      $( '.waitlist-modal' ).addClass( 'show' );

      $( 'html' ).addClass( 'wait-no-scroll' );

      let json_str=$('.product-title').closest('[data-js-product]').attr('data-json-product');
      let json=JSON.parse(json_str);
      let size = $('[data-property=size] .checkbox.active')[0].innerText,
              set = $('[data-property=set] .checkbox.active')[0].innerText;
      $.each(json.variants, function () {
        if (this.option1 === size && this.option2 ===set){
          id=this.id;
        }
      });
    });
    $( document ).on( 'click', '.js-close-waitlist', function() {
        $( 'html' ).removeClass( 'wait-no-scroll' );
        $( '.waitlist-modal' ).removeClass( 'show' );
    });
    $( document ).click( function( e ) {
        if ( $( e.target ).closest( '.js-show-waitlist, .waitlist-modal__inner' ).length )
            return;

        $( '.waitlist-modal' ).removeClass( 'show' );
      $( 'html' ).removeClass( 'wait-no-scroll' );

      e.stopPropagation();
    });

    $(".waitlist-modal__options .checkbox").on('click',function (){
      let attr =$(this).parent($('.checkbox-list')).attr('data-property');
      $(`[data-property=${attr}] .checkbox`).removeClass('active');
      $(this).addClass('active');
      let json_str=$('.product-title').closest('[data-js-product]').attr('data-json-product');
      let json=JSON.parse(json_str);
      let size = $('[data-property=size] .checkbox.active')[0].innerText,
              set = $('[data-property=set] .checkbox.active')[0].innerText;
      $.each(json.variants, function () {
        if (this.option1 === size && this.option2 ===set){
          id=this.id;
        }
      });

    });

    $("#waitlistForm").submit(function (){
      if($('#addEmail').prop('checked')) {
        let email=$('.form-group #email').val();
        $.ajax({
          type:'POST',
          url:'https://a.klaviyo.com/onsite/components/back-in-stock/subscribe',
          data:{
            a:'PAgcrr',
            email: email,
            variant:id,
            platform:'shopify'
          },
          success:function (response){
            console.log(response)
          }

        })
        KlaviyoSubscribe.attachToModalForm('#product_waitlist',
                {
                  hide_form_on_success: false,
                  ignore_cookie: true,
                  success_message: false,

                });

        $(".klav_email").val(email);
        setTimeout(function (){
          $('.klaviyo_subscription_form').trigger('submit')
          setTimeout(function () {
            $('.klaviyo_subscription_form').trigger('reset')
          }, 500)
        }, 500)
      }
    })
</script>
