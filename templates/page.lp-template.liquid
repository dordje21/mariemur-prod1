<div class="wrapper lp-wrapper js-scrollbar test">
    {% section 'header-lp' %}
    <main class="main">
        {% section 'page-lp-home' %}
        {% section 'page-lp-featured' %}
        {% section 'page-lp-about' %}
        {% section 'page-lp-shop' %}
        {% section 'shop-countdown-timer' %}
        {% section 'page-lp-catalog' %}
        {% section 'page-lp-feedbacks' %}
        {% section 'page-lp-subscription' %}
        {% section 'footer-lp' %}
    </main>
</div>

<!-- Custom Cursor Pointer -->
<div class="cursor">
    <svg viewBox="0 0 40 40">
        <circle cx="20" cy="20" r="18" stroke-width="1" stroke="currentColor"></circle>
    </svg>
</div>

<!-- Custom Page Preload -->
<div class="preload">
    <div class="preload-box">
        <div class="preload-box__circle">
            <svg viewBox="0 0 302 302" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="151" cy="151" r="150" stroke="#A2A2A2" stroke-opacity="0.3" stroke-width="1" stroke-dashoffset="1300" stroke-dasharray="1300" />
            </svg>
        </div>
        {% if template == "page.lp-template" %}
      		{% if settings.preloader_image_custom.size > 0 %}
      			<img src="{{ settings.preloader_image_custom | img_url: "master" }}" alt="{{ shop.name }}">
      		{% else %}
      			 <img src="{{ 'lp_logo_white.png' | asset_url }}" alt="{{ shop.name }}">
      		{% endif %}
      {% endif %}
       
    </div>
</div>