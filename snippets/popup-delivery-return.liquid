<div class="popup-delivery-return position-relative py-30 px-15" data-popup-content>
    <i class="popup-delivery-return__close position-absolute cursor-pointer" data-js-popup-close>{% include 'icon-theme-164' %}</i>
    <div class="popup-delivery-return__content mx-auto">
        <div class="rte">
            {%- if section.settings.delivery_return_page_content != blank -%}
                {% include 'content-load-template' content: pages[section.settings.delivery_return_page_content].content %}
            {%- else -%}
                {% include 'content-load-template' content: section.settings.delivery_return_content %}
            {%- endif -%}
        </div>
    </div>
</div>